@model IEnumerable<HarmonyWebApp.Models.Database.Activity>
@{
    ViewBag.Title = "LoggedIn";
}

@if (Session["UserId"] != null)
{
    <h3>Zalogowany jako:  @Session["UserName"].ToString()</h3>
}

<div>
    @foreach (var item in Model)
    {
        <div>
            <h3>@item.name</h3>
            @item.description
            <h4>Start: @item.start_date</h4>
        </div>
    }
</div>

